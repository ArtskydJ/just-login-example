{
  "name": "just-login-example",
  "version": "0.0.1",
  "main": "./server.js",
  "author": "ArtskydJ <josephdykstra@gmail.com>",
  "description": "Browser code for the just-login-core and just-login-emailer",
  "keywords": [
    "just login"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/coding-in-the-wild/just-login-example"
  },
  "devDependencies": {
    "level-mem": "^0.18.0",
    "superagent": "^0.18.0",
    "tap": "~0.4.8"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node test.js",
    "build": "browserify client-src/index.js -d -o static/build.js",
    "watch": "watchify client-src/index.js -d -o static/build.js",
    "deploy": "nohup ploy server ./data -p 80 -a ../auth.json",
    "redeploy": "git push ploy master",
    "prestart": "browserify client-src/index.js | uglifyjs -m -o static/build.js"
  },
  "dependencies": {
    "confuse": "0.0.1",
    "dnode": "^1.2.0",
    "domready": "^1.0.5",
    "email-validator": "^1.0.1",
    "just-login-client": "^1.2.2",
    "just-login-core": "^1.2.3",
    "just-login-debouncer": "^1.3.0",
    "just-login-emailer": "^1.1.0",
    "just-login-server-api": "^1.0.3",
    "level": "^0.18.0",
    "level-spaces": "^1.1.1",
    "ms": "^0.6.2",
    "node-static": "^0.7.6",
    "ractive": "^0.5.0",
    "run-waterfall": "^1.0.0",
    "shoe": "0.0.15",
    "weak": "^0.3.3",
    "xtend": "^3.0.0"
  },
  "bugs": {
    "url": "https://github.com/coding-in-the-wild/just-login-example/issues"
  },
  "homepage": "https://github.com/coding-in-the-wild/just-login-example"
