  "scripts": {
    "test": "node test/test.js",
    "build": "browserify client-src/index.js -d -o static/build.js",
    "watch": "watchify client-src/index.js -d -o static/build.js",
    "start": "nohup node . 1028 &",
    "stop": "kill $(ps x | awk '/node \\. 1028/ {print $1}') || echo ''",
    "poststart": "browserify client-src/index.js | uglifyjs -m -o static/build.js && node build/putInfoIntoHtml.js"
  },
